{
  "name": "crystalkeepsakes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/fetch-cockpit3d-products.js",
    "build": "next build && node scripts/copy-api.js && cp .env.production out/.env",
    "build:test": "env-cmd -f .env.production.test next build && node scripts/copy-api.js && cp .env.production.test out/.env",
    "build:prod": "env-cmd -f .env.production.root next build && node scripts/copy-api.js && cp .env.production.root out/.env",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.0.0",
